<Application x:Class="EMC07.ControlsUI.App"
			 xmlns="https://github.com/avaloniaui"
             xmlns:presentation="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:EMC07.ControlsUI"
             Startup="AppStart">
	<Application.Resources>

		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="ResourceDictionaries/Constants/GlobalConstants.axaml"/>
				<ResourceDictionary Source="ResourceDictionaries/Constants/Strings.axaml"/>
				<ResourceDictionary Source="ResourceDictionaries/LightTheme/LightTheme.axaml"/>
				<ResourceDictionary Source="ResourceDictionaries/Styles/OtherStyles.axaml"/>
				<ResourceDictionary Source="ResourceDictionaries/Styles/ListBoxStyles.axaml"/>
				<ResourceDictionary Source="ResourceDictionaries/Styles/ButtonStyles.axaml"/>
				<ResourceDictionary Source="ResourceDictionaries/Styles/InputStyles.axaml"/>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>

	</Application.Resources>
</Application>
